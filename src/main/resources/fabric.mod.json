{
  "schemaVersion": 1,
  "id": "exeter-addon",
  "version": "${version}",
  "name": "Exeter Addon",
  "description": "Meteor Client theme based on Exeter's GUI.",
  "authors": [
    "Sophimoo"
  ],
  "contact": {
    "sources": "https://github.com/sophimoo/Exeter-Addon"
  },
  "icon": "assets/base-addon/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "me.sophimoo.exeter.BaseAddon"
    ]
  },
  "mixins": [
    "base.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "90,140,90"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
